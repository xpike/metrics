{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "XPike": {
    "Logging": {
      "Console": {
        "ConsoleLogConfig": {
          "ShowStackTraces": true,
          "ShowMetadata": false,
          "Enabled": true
        }
      },
      "Debug": {
        "DebugLogConfig": {
          "Enabled": false
        }
      },
      "File": {
        "FileLogConfig": {
          "Enabled": false
        }
      },
      "LogServiceConfig": {
        "MaxQueueLength": 1000,
        "EnqueueTimeoutMs": 150,
        "LogLevel": "Trace" 
      } 
    },
    "Metrics": {
      "MetricsConfig": {
        "Prefix": "app.example",
        "ConstantTags": [ "example:xpikemetrics22" ],
        "MaxMessagesPerPayload": 50,
        "TrackAttempts": false,
        "TrackResults": false,
        "TrackTiming": true
      },
      "DataDog": {
        "DataDogProviderConfig": {
          "StatsdServerName": "127.0.0.1",
          "StatsdPort": 8125,
          "StatsdMaxUDPPacketSize": 512,
          "StatsdTruncateIfTooLong": true
        }
      }
    }
  } 
}